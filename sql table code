CREATE TABLE students (
    ID int,
    Name varchar(255),
    Surname varchar(255),
    Age varchar(255),
    Class_ID varchar(255),
    GPA varchar(255)
);

INSERT INTO students (ID, Name, Surname, Age, Class_ID, GPA)
VALUES
    (1, 'Jack', "Phillips", 20, 1, 3.5),
    (2, 'Sam', "Dimmons", 19, 1, 3.2),
    (3, 'Patty', "Millions", 22, 2, 1.2),
    (4, 'Harry', "Potter", 18, 2, 4.0);
 
